month={Feb},}
@ARTICLE{8924662,
author={X. {Chen} and Y. {Yuan} and L. {Lu} and J. {Yang}},
journal={IEEE Access}, title={A Multidimensional Trust Evaluation Framework for Online Social Networks Based on Machine Learning},
year={2019},
