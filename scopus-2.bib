@CONFERENCE{Rethishkumar20201170,
author={Rethishkumar, S. and Vijayakumar, R.},
title={Stackelberg Model with MFO mitigate Co-RDoS threats in Cloud servers},
journal={Proceedings of the International Conference on Intelligent Computing and Control Systems, ICICCS 2020},
year={2020},
pages={1170-1177},
doi={10.1109/ICICCS48265.2020.9121149},
art_number={9121149},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85087447079&doi=10.1109%2fICICCS48265.2020.9121149&partnerID=40&md5=2e1c3fccd5c45e3773c5c81e69209ca1},
abstract={Cloud computing is a trend in technology that stores and retrieves valuable data via worldwide distributed servers. Various virtual machines (VMs) share the same physical resources across these services; these VMs are called Co-VMs. The shared physical resources pose a major challenge to the users. A tool may belong to both professional organizing as well as unknown attackers. There's no guarantee, from a cloud user's viewpoint, that the co-resident VMs are trusted. Shared resources establish the unplausible privacy and ideal separations where it paves the gate for the invasion of co-residents, where a VM invades other co-resident VM. Here an improved relevance vector machine with fuzzy c-means clustering (IRVM-FCM) depending defense systems are discussed through reducing the Co-location DOS invaders by assuring it hardening for attackers to make an attack. By this method, primarily, the attacker behavior is verified through using the previously selected server first (PSSF) VM allocation strategy. After, the partial labeling is achieved using FCM Clustering system; it is used to categorize users as legal or malicious. After that, an IRVM classification scheme is discussed for varying all users with three categories like high risk (i.e. malicious), medium risk (i.e. uncertain), and low risk (genuine). Within IRVM, the Mosquito Flying behavior depending on the swarm intelligence optimization approach used to optimize the kernel functions of parameters to enhance the training methods. Finally, Stackelberg game theory is adapted to hike the price of launching a new VM hence diminish the possibility of a Co-R DOS threat in cloud servers. The experimental implementation with Cloudsim results displays that the discussed IRVM-FCM method attained highperformance results compared to previous VM allocation schemes. © 2020 IEEE.},
author_keywords={Cloud Computing;  Co-Resident DoS attack;  FCM;  IRVM;  MFO;  PSSF;  Stackelberg game model;  VM allocation policies},
document_type={Conference Paper},
source={Scopus},
}

@ARTICLE{Wang2020152,
